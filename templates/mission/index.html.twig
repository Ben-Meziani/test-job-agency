{% extends 'base.html.twig' %}

{% block title %}Toutes les missions{% endblock %}

{% block body %}
    <div class="container">
      {% for mission in missions %}
      <div class="col-md-4">
        <div class="card">
          <div class="content">
            <div class="title">{{ mission.title }}</div>
            <div class="sub-title">{{ mission.city }}</div>
            <p>{{ mission.description }}</p>
            <div class="btn"><button>Read more</button></div>
        </div>
    </div>
    </div>
     {% endfor %}
    </div>


{% endblock %}
